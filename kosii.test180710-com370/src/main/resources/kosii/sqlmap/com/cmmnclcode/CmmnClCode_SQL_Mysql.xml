<sqlMap namespace="CmmnClCode">

	<select id="CmmnClCodeDAO.selectCmmnClCode" parameterClass="kosii.com.cmmnclcode.service.CmmnClCodeVO"
		resultClass="egovframework.rte.psl.dataaccess.util.EgovMap">
