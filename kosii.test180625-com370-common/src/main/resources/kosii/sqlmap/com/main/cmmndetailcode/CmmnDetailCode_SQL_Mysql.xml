<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apche.org/dtd/sql-map-2.dtd">

<sqlMap namespace="CmmnDetailCode">

	<select
		id="kosii.com.main.cmmndetailcode.service.impl.CmmnDetailCodeDAO.selectCmmnDetailCodeList"
		parameterClass="egovframework.com.cmm.ComDefaultCodeVO" resultClass="egovframework.rte.psl.dataaccess.util.EgovMap">
<![CDATA[ 
SELECT 
	CODE_ID
	, CODE
	, CODE_NM
	, CODE_DC
FROM COMTCCMMNDETAILCODE
WHERE USE_AT = 'Y'
	AND CODE_ID = #codeId#
]]>
	</select>

</sqlMap>
